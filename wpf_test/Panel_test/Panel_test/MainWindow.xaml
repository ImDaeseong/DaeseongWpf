<Window x:Class="Panel_test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Panel_test"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel Orientation="Vertical">
            <Button Content="Canvas" Width="200" Height="50" Click="Canvas_Click"/>
            <Button Content="DockPanel"  Width="200" Height="50" Click="DockPanel_Click"/>
            <Button Content="Expander"  Width="200" Height="50" Click="Expander_Click" />
            <Button Content="GridSplitter"  Width="200" Height="50" Click="GridSplitter_Click" />
            <Button Content="StackPanel"  Width="200" Height="50" Click="StackPanel_Click"/>
            <Button Content="UniformGrid"  Width="200" Height="50" Click="UniformGrid_Click"/>
            <Button Content="WrapPanel"  Width="200" Height="50" Click="WrapPanel_Click"/>
            <Button Content="TabControl"  Width="200" Height="50" Click="TabControl_Click"/>
        </StackPanel>
    </Grid>
    
</Window>
